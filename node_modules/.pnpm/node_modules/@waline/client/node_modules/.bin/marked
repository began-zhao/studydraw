#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/blog/gitblog/studydraw/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/blog/gitblog/studydraw/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+marked@4.0.18/node_modules/marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+marked@4.0.18/node_modules/marked/bin/marked.js" "$@"
fi
